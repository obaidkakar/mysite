<p id="notice"><%= notice %></p>

<h1>St Has Deps</h1>

<table>
  <thead>
    <tr>
      <th>Department</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @st_has_deps.each do |st_has_dep| %>
      <tr>
        <td><%= st_has_dep.department %></td>
        <td><%= st_has_dep.student %></td>
        <td><%= link_to 'Show', st_has_dep %></td>
        <td><%= link_to 'Edit', edit_st_has_dep_path(st_has_dep) %></td>
        <td><%= link_to 'Destroy', st_has_dep, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

